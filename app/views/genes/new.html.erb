<h1>New gene</h1>

<% form_for(@gene) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :taxonomy_id %><br />
    <%= f.text_field :taxonomy_id %>
  </p>
  <p>
    <%= f.label :symbol %><br />
    <%= f.text_field :symbol %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', genes_path %>