<h1><%=h @bibliome.name %>: <%= @bibliome.status + " " + @bibliome.articles_count.to_s + " of " + @count.to_s + " articles in " + processing_time(@bibliome) %></h1>

<table>
  <tr>
    <th>ID</th>
    <th>LAST ERROR</th>
    <th>JOB</th>
    <th>RUN AT</th>
    <th>CREATED AT</th>
  </tr>
<% Delayed::Job.find(:all).each do |j| %>
  <tr>
    <td><%= j.id %></td>
    <td><%= j.last_error %></td>
    <td><%= j.handler %></td>
    <td><%= j.run_at %></td>
    <td><%= j.created_at %></td>
  </tr>
<% end %>
</table>